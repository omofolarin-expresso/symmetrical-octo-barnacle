{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node ./node_modules/jest/bin/jest.js --ci --reporters=default --reporters=jest-junit",
    "test:watch": "node ./node_modules/jest/bin/jest.js --watchAll "
